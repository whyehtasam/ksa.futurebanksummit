{
    "rewrites": [
      {
        "source": "/(schedule|speakers)/",
        "destination": "/index.html"
      },
      {
        "source": "/(.*)",
        "destination": "/index.html"
      }
    ],
    "routes": [
      {
        "handle": "filesystem"
      },
      {
        "src": ".*",
        "dest": "/404.html"
      }
    ]
  }